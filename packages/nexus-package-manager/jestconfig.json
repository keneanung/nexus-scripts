{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/lib/"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "reporters": [
    "default",
    "github-actions"
  ],
  "automock": false,
  "resetMocks": false,
  "testEnvironment": "jsdom"
}